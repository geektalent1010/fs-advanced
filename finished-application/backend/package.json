  "dependencies": {
    "@keystone-6/auth": "^1.0.0",
    "@keystone-6/cloudinary": "^1.0.0",
    "@keystone-6/core": "^1.0.0",
    "@prisma/client": "3.6.0",
    "@types/nodemailer": "^6.4.4",
    "dotenv": "^10.0.0",
    "nodemailer": "^6.7.2",
    "stripe": "^8.191.0"
